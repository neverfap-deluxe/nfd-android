<?xml version="1.0" encoding="UTF-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:left="40dp" android:bottom="40dp">
        <shape
                android:shape="rectangle">
            <padding android:left="20dp" android:right="20dp" android:top="20dp" android:bottom="20dp"/>
            <stroke android:width="4dp" android:color="#FFFFFF"/>
            <!--<solid android:color="#CF0F0F"/>-->
            <gradient
                    android:angle="45"
                    android:endColor="@color/gradientEndColor"
                    android:startColor="@color/gradientStartColor"/>

        </shape>
    </item>

    <item android:id="@+id/innerRectangle">
        <shape
                android:shape="rectangle">
            <padding android:left="20dp" android:right="20dp" android:top="20dp" android:bottom="20dp"/>
            <stroke android:width="4dp" android:color="#FFFFFF"/>
            <solid android:color="#000000"/>
        </shape>
    </item>
</layer-list>
